<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<xsd:element name="EMPLOYEES" type="complexTypeEmployees" />
<xsd:complexType name="complexTypeEmployees">
	<xsd:sequence>
		<xsd:element name="EMPLOY" type="complexTypeEmploy" maxOccurs="unbounded"/>
	</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="complexTypeEmploy">
	<xsd:sequence>
		<xsd:element name="EMPNO" type="simpleTypeEmpno"/>
		<xsd:element name="NAME" type="simpleTypeName"/>
		<xsd:element name="BASIC" type="simpleTypeBasic"/>
	</xsd:sequence>
</xsd:complexType>
<xsd:simpleType name="simpleTypeEmpno">
	<xsd:restriction base="xsd:positiveInteger">
		<xsd:minInclusive value="1"/>
		<xsd:maxInclusive value="10"/>
	</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="simpleTypeName">
	<xsd:restriction base="xsd:string">
		<xsd:pattern value="KRISHNA|MURALI|ANUSHKA|AKASH"/>
	</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="simpleTypeBasic">
	<xsd:restriction base="xsd:positiveInteger">
		<xsd:minInclusive value="1000"/>
		<xsd:maxInclusive value="50000"/>
	</xsd:restriction>
</xsd:simpleType>
</xsd:schema>